# ```python
import pandas as pd
from sklearn.preprocessing import OneHotEncoder
from sklearn.model_selection import train_test_split
from sklearn.ensemble import RandomForestClassifier
from sklearn.metrics import accuracy_score, f1_score, roc_auc_score

train_data = pd.read_csv("../../../data/Airlines/Airlines_train.csv")
test_data = pd.read_csv("../../../data/Airlines/Airlines_test.csv")



ohe = OneHotEncoder(handle_unknown='ignore')
feature_cols = ['DayOfWeek', 'Airline']
ohe.fit(pd.concat([train_data[feature_cols], test_data[feature_cols]]))
train_ohe = ohe.transform(train_data[feature_cols]).toarray()
test_ohe = ohe.transform(test_data[feature_cols]).toarray()

train_data = pd.concat([train_data, pd.DataFrame(train_ohe)], axis=1)
test_data = pd.concat([test_data, pd.DataFrame(test_ohe)], axis=1)

train_data['AirportFrom_To'] = train_data['AirportFrom'] * train_data['AirportTo']
test_data['AirportFrom_To'] = test_data['AirportFrom'] * test_data['AirportTo']

train_data['DayOfWeek_Time'] = train_data['DayOfWeek'] * train_data['Time']
test_data['DayOfWeek_Time'] = test_data['DayOfWeek'] * test_data['Time']

train_data.drop(columns=['Flight', 'DayOfWeek', 'Airline'], inplace=True)
test_data.drop(columns=['Flight', 'DayOfWeek', 'Airline'], inplace=True)

X_train = train_data.drop('Delay', axis=1)
y_train = train_data['Delay']
X_test = test_data.drop('Delay', axis=1)
y_test = test_data['Delay']

trn = RandomForestClassifier(max_leaf_nodes=500, random_state=42)
trn.fit(X_train, y_train)

Train_Accuracy = accuracy_score(y_train, trn.predict(X_train))
Test_Accuracy = accuracy_score(y_test, trn.predict(X_test))

Train_F1_score = f1_score(y_train, trn.predict(X_train))
Test_F1_score = f1_score(y_test, trn.predict(X_test))

Train_AUC = roc_auc_score(y_train, trn.predict_proba(X_train)[:, 1])
Test_AUC = roc_auc_score(y_test, trn.predict_proba(X_test)[:, 1])

print(f"Train_AUC:{Train_AUC}")
print(f"Train_Accuracy:{Train_Accuracy}")   
print(f"Train_F1_score:{Train_F1_score}")
print(f"Test_AUC:{Test_AUC}")
print(f"Test_Accuracy:{Test_Accuracy}")   
print(f"Test_F1_score:{Test_F1_score}") 
# ```end