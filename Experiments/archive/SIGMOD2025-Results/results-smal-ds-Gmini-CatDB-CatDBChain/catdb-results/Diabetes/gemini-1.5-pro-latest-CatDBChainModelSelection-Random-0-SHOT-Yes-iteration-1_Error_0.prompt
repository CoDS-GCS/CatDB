    bmi = X[:, 0]
    age = X[:, 1]
    return np.sqrt(bmi) * age.reshape(-1, 1)  # Example interaction, adjust as needed

def calculate_glucose_insulin(X):
    glucose = X[:, 0]
    insulin = X[:, 1]
    return (glucose / insulin).reshape(-1, 1)

