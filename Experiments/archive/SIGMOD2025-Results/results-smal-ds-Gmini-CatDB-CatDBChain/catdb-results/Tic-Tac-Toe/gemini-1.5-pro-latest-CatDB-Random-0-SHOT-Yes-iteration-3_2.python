# ```python
import pandas as pd
from sklearn.preprocessing import OneHotEncoder
from sklearn.ensemble import RandomForestClassifier
from sklearn.metrics import accuracy_score, f1_score, roc_auc_score
import numpy as np

train_data = pd.read_csv("../../../data/Tic-Tac-Toe/Tic-Tac-Toe_train.csv")
test_data = pd.read_csv("../../../data/Tic-Tac-Toe/Tic-Tac-Toe_test.csv")


def augment_data(df):
    """
    Augments the tic-tac-toe data by generating rotations and reflections of each board configuration.
    """
    augmented_data = []
    for i in range(len(df)):
        board = df.iloc[i].values[:-1].reshape(3, 3)
        for _ in range(4):
            board = np.rot90(board)
            augmented_data.append(np.append(board.flatten(), df.iloc[i].values[-1]))
        board = np.fliplr(board)
        augmented_data.append(np.append(board.flatten(), df.iloc[i].values[-1]))
        board = np.flipud(board)
        augmented_data.append(np.append(board.flatten(), df.iloc[i].values[-1]))
    return pd.DataFrame(augmented_data, columns=df.columns)

train_data = augment_data(train_data)

columns_to_encode = ['bottom-middle-square', 'top-middle-square', 'bottom-left-square', 'middle-left-square',
                   'bottom-right-square', 'top-right-square', 'middle-right-square', 'middle-middle-square',
                   'top-left-square']
enc = OneHotEncoder(handle_unknown='ignore')
enc.fit(pd.concat([train_data[columns_to_encode], test_data[columns_to_encode]]))
train_encoded = pd.DataFrame(enc.transform(train_data[columns_to_encode]).toarray())
test_encoded = pd.DataFrame(enc.transform(test_data[columns_to_encode]).toarray())
train_data = train_data.reset_index(drop=True).join(train_encoded)
test_data = test_data.reset_index(drop=True).join(test_encoded)


target_variable = 'Class'

trn = RandomForestClassifier(max_leaf_nodes=500)
trn.fit(train_data.drop(columns=[target_variable]), train_data[target_variable])

Train_AUC = roc_auc_score(train_data[target_variable], trn.predict_proba(train_data.drop(columns=[target_variable]))[:, 1])
Train_Accuracy = accuracy_score(train_data[target_variable], trn.predict(train_data.drop(columns=[target_variable])))
Train_F1_score = f1_score(train_data[target_variable], trn.predict(train_data.drop(columns=[target_variable])))
Test_AUC = roc_auc_score(test_data[target_variable], trn.predict_proba(test_data.drop(columns=[target_variable]))[:, 1])
Test_Accuracy = accuracy_score(test_data[target_variable], trn.predict(test_data.drop(columns=[target_variable])))
Test_F1_score = f1_score(test_data[target_variable], trn.predict(test_data.drop(columns=[target_variable])))

print(f"Train_AUC:{Train_AUC}")
print(f"Train_Accuracy:{Train_Accuracy}")
print(f"Train_F1_score:{Train_F1_score}")
print(f"Test_AUC:{Test_AUC}")
print(f"Test_Accuracy:{Test_Accuracy}")
print(f"Test_F1_score:{Test_F1_score}")
# ```end